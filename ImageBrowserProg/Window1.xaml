<Window x:Class="ImageBrowserProg.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageBrowserProg"
        mc:Ignorable="d"
        Title="Window1" Height="auto" Width="auto" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    
    <Grid Height="auto" Width="auto">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="image" Source="{Binding}" Grid.Row="0" Grid.Column="0" Margin="0,0,0,0" Width="auto" Height="auto" Stretch="UniformToFill"  />
    </Grid>
</Window>
